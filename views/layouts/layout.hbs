        <div class="image-section" id="project-logo">
            <img src="/chrome_topten_horz_light.png" />
        </div>

        <!-- The main heading area -->
        <div class="inner">
            <a href="/">Responsive Paper</a> <small>v0.1</small>
        </div>

        <!-- Links shown in the top-right hand corner of the page -->
        <div id="header-links">
            <a href="https://www.responsivepaper.com">Responsive Paper</a>
            <a href="https://github.com/responsivepaper">GitHub</a>
